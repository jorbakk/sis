MINGW_CC     = x86_64-w64-mingw32-gcc
MINGW_CXX    = x86_64-w64-mingw32-c++
MINGW_AR     = x86_64-w64-mingw32-ar
MINGW_CFLAGS = -I. -O2 -DRELEASE -Isys.x86_64-w64-mingw32/include
MINGW_LDLIBS = 
